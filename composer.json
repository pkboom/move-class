    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "symfony/console": "^5.4|^6.0",
        "symfony/finder": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.0|^6.0"
